{
  "name": "TelevisionForReddit",
  "version": "1.0.0",
  "author": "Ricardo Valencia",
  "description": "Automatically play through videos on /r/videos.",
  "scripts": {
    "start": "node ./dist/server.js",
    "dev": "cross-env NODE_ENV=development babel-node ./src/server.js",
    "clean": "rm -rf dist",
    "build": "npm run clean && npm run build:client & npm run build:server",
    "build:client": "cross-env NODE_ENV=production webpack --config ./webpack.config.prod.js --progress --profile --colors",
    "build:server": "cross-env NODE_ENV=production webpack --config ./webpack.config.server.js --progress --profile --colors",
    "test": "cross-env NODE_ENV=test mocha --compilers js:babel-core/register --require ./tests/testHelper.js --recursive ./tests",
    "test:watch": "npm run test -- --watch",
    "count": "cloc --exclude-dir=node_modules,.idea,.git --exclude-list-file=.env,.DS_STORE ."
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-1": "^6.1.18",
    "cloc": "^2.2.0",
    "cross-env": "^3.1.4",
    "css-loader": "^0.26.1",
    "html-webpack-plugin": "^2.28.0",
    "style-loader": "^0.13.1",
    "webpack": "^2.2.1",
    "webpack-dev-middleware": "^1.10.0",
    "webpack-hot-middleware": "^2.16.1"
  },
  "dependencies": {
    "axios": "^0.15.3",
    "chai": "^4.0.2",
    "debug": "^2.6.0",
    "dotenv": "^4.0.0",
    "express": "^4.14.1",
    "jsdom": "^11.0.0",
    "lodash": "^4.17.4",
    "material-ui": "^0.18.4",
    "mocha": "^3.4.2",
    "morgan": "^1.8.0",
    "qs": "^6.4.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-player": "^0.18.0",
    "react-redux": "^5.0.5",
    "react-router": "^3.0.2",
    "react-router-redux": "^4.0.8",
    "react-tap-event-plugin": "^2.0.1",
    "redux": "^3.7.1",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.0",
    "shortid": "^2.2.8"
  }
}
